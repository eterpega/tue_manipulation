
<launch> 
	<!-- send parameters for collision checking for PR2; this includes parameters for the self filter -->
	<rosparam command="load" ns="robot_description_collision" file="$(find amigo_arm_navigation_config)/config/collision_checks_both_arms.yaml" />

	<!-- send parameters needed for motion planning -->
	<rosparam command="load" ns="robot_description_planning" file="$(find amigo_arm_navigation_config)/config/planning_groups.yaml" />

</launch>
