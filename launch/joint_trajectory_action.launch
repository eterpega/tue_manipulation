<?xml version="1.0"?>
<launch>
    <include file="$(find amigo_machine_files)/machines/amigo4.machine" />

    <node pkg="amigo_arm_navigation" type="joint_trajectory_action" name="joint_trajectory_action_left" output="screen" machine="amigo4">
	<rosparam command="load" file="$(find amigo_arm_navigation)/config/joints_left.yaml" />
        <remap from="joint_trajectory_action" to="joint_trajectory_action_left" />
        <remap from="joint_measurements" to="/arm_left_controller/joint_measurements" />
	<remap from="joint_references" to="/arm_left_controller/joint_references" />
    </node>

    <node pkg="amigo_arm_navigation" type="joint_trajectory_action" name="joint_trajectory_action_right" output="screen" machine="amigo4">
	<rosparam command="load" file="$(find amigo_arm_navigation)/config/joints_right.yaml" />
        <remap from="joint_trajectory_action" to="joint_trajectory_action_right" />
        <remap from="joint_measurements" to="/arm_right_controller/joint_measurements" />
	<remap from="joint_references" to="/arm_right_controller/joint_references" />
    </node>
</launch>
