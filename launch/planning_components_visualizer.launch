<launch>
    <param name="robot_description" command="$(find xacro)/xacro.py '$(find amigo_robot_model)/description/robot_$(optenv ROBOT_MODEL_COMPLEXITY).urdf.xacro' " />
    <rosparam command="load" ns="robot_description_planning" file="$(find amigo_arm_navigation)/config/amigo_planning_description_$(optenv ROBOT_MODEL_COMPLEXITY).yaml"/>

    <include file="$(find planning_environment)/launch/planning_environment_visualization_prerequisites.launch" />
    <include file="$(find amigo_arm_navigation)/launch/constraint_aware_kinematics.launch" />
    <include file="$(find amigo_arm_navigation)/launch/ompl_planning.launch" />
    <include file="$(find amigo_arm_navigation)/launch/trajectory_filter_server.launch" />
    <node pkg="rviz" type="rviz" name="rviz_planning_components" args="-d $(find amigo_arm_navigation)/config/planning_components_visualizer.vcg" />
    <node pkg="move_arm" type="planning_components_visualizer" name="planning_components_visualizer" output="screen" />
    <node pkg="robot_state_publisher" type="state_publisher" name="rob_st_pub" />
</launch>
