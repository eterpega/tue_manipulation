<launch>
        <node pkg="arm_kinematics_constraint_aware" type="arm_kinematics_constraint_aware" name="amigo_left_arm_kinematics" output='screen'>
        <param name="group" type="string" value="left_arm" />
        <param name="left_arm/root_name" type="string" value="torso" />
        <param name="left_arm/tip_name" type="string" value="grippoint_left" />
        <param name="kinematics_solver" type="string" value="arm_kinematics_constraint_aware/KDLArmKinematicsPlugin" />
