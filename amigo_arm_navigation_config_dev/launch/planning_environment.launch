
<launch> 
	<!-- send parameters for collision checking; this includes parameters for the self filter -->
	<rosparam command="load" ns="robot_description_collision" file="$(find amigo_arm_navigation_config_dev)/config/collision_checks_both_arms.yaml" />

	<!-- send parameters for multidof -->
	<rosparam command="load" ns="robot_description_planning" file="$(find amigo_arm_navigation_config_dev)/config/multi_dof_joints.yaml" />

	<!-- send parameters needed for motion planning -->
	<rosparam command="load" ns="robot_description_planning" file="$(find amigo_arm_navigation_config_dev)/config/planning_groups.yaml" />

</launch>
